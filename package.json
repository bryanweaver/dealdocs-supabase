{
  "name": "dealdocs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "test": "vitest --config vite.config.js --dom",
    "test:ui": "vitest --config vite.config.js --dom --ui",
    "tailwind": "npx tailwindcss -i ./src/style.css -o ./src/output.css --watch",
    "generate-state": "node ./src/store/generateInitialState.js",
    "compile-gql": "amplify api gql-compile",
    "reduce-agents": "node reduceAgents.js",
    "playwright:mcp": "npx -y @playwright/mcp@latest --port 5123",
    "test:playwright": "concurrently -k -s first \"npm run playwright:mcp\" \"npx playwright test\"",
    "bdd": "node --loader ts-node/esm node_modules/.bin/cucumber-js src/tests/**/*.feature --import 'src/tests/steps/*.ts' --format @cucumber/pretty-formatter",
    "test:login": "node --loader ts-node/esm node_modules/.bin/cucumber-js src/tests/login.feature --import 'src/tests/steps/*.ts' --format @cucumber/pretty-formatter",
    "test:property-search": "node --loader ts-node/esm node_modules/.bin/cucumber-js src/tests/property-search.feature --import 'src/tests/steps/*.ts' --format @cucumber/pretty-formatter",
    "scrape:addresses": "node src/tests/utils/address-manager.js",
    "test:systematic": "node src/tests/runners/systematic-test-runner.js",
    "test:systematic:3": "node src/tests/runners/systematic-test-runner.js 3",
    "test:systematic:10": "node src/tests/runners/systematic-test-runner.js 10",
    "test:systematic:all": "node src/tests/runners/systematic-test-runner.js 50"
  },
  "dependencies": {
    "@anvilco/anvil": "^3.3.1",
    "@aws-amplify/ui-vue": "^4.3.3",
    "@aws-sdk/client-dynamodb": "^3.806.0",
    "@aws-sdk/credential-providers": "^3.806.0",
    "@aws-sdk/lib-dynamodb": "^3.806.0",
    "@playwright/mcp": "^0.0.28",
    "@primevue/themes": "^4.2.4",
    "@vuepic/vue-datepicker": "^8.7.0",
    "aws-amplify": "^6.0.13",
    "aws-sdk": "^2.1691.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "file-icon-vectors": "^1.0.0",
    "floating-vue": "^5.2.2",
    "happy-dom": "^13.3.1",
    "papaparse": "^5.5.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.4",
    "smartystreets-javascript-sdk": "^5.1.4",
    "uuid": "^11.1.0",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0",
    "vuex-persistedstate": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.3.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@playwright/test": "^1.53.0",
    "@primevue/auto-import-resolver": "^4.0.7",
    "@types/smartystreets-javascript-sdk": "^5.1.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cssnano": "^7.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.20.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "sass": "^1.79.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-primeui": "^0.3.4",
    "terser": "^5.43.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.0.8",
    "vitest": "^1.2.2"
  }
}
