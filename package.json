{
  "name": "dealdocs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-types": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:playwright": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "bdd": "cucumber-js",
    "test:login": "cucumber-js src/tests/login.feature",
    "test:property-search": "cucumber-js src/tests/property-search.feature",
    "test:systematic": "tsx src/tests/runners/systematic-test-runner.ts --addresses 10",
    "test:systematic:3": "tsx src/tests/runners/systematic-test-runner.ts --addresses 3",
    "test:systematic:all": "tsx src/tests/runners/systematic-test-runner.ts --addresses 50",
    "test:e2e": "playwright test src/tests/e2e --project=chromium",
    "test:e2e:all": "playwright test src/tests/e2e",
    "test:e2e:headed": "playwright test src/tests/e2e --project=chromium --headed",
    "test:e2e:debug": "playwright test src/tests/e2e --project=chromium --debug",
    "test:auth": "playwright test src/tests/e2e/auth-test.spec.ts --project=chromium",
    "test:workflow": "playwright test src/tests/e2e/full-workflow-test.spec.ts --project=chromium",
    "test:workflow:headed": "playwright test src/tests/e2e/full-workflow-test.spec.ts --project=chromium --headed",
    "test:all": "npm run test && npm run test:e2e",
    "test:report": "playwright show-report",
    "test:report:html": "playwright test src/tests/e2e --reporter=html",
    "test:report:json": "playwright test src/tests/e2e --reporter=json > test-results/report.json",
    "test:report:summary": "playwright test src/tests/e2e --reporter=list",
    "generate-state": "node src/utils/generate-state.js",
    "scrape:addresses": "tsx src/tests/utils/scrape-addresses.ts",
    "playwright:mcp": "npx playwright-mcp-server",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:db:push": "supabase db push",
    "supabase:db:reset": "supabase db reset",
    "supabase:functions:deploy": "supabase functions deploy",
    "supabase:deploy": "powershell -ExecutionPolicy Bypass -File deploy-to-supabase.ps1",
    "db:generate-types": "supabase gen types typescript --project-id rimwbejtyzolicucxbec > src/types/supabase.ts"
  },
  "dependencies": {
    "@primevue/themes": "^4.1.0",
    "@supabase/supabase-js": "^2.49.1",
    "@vueuse/core": "^11.2.0",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "file-icon-vectors": "^1.0.0",
    "floating-vue": "^5.2.2",
    "jwt-decode": "^4.0.0",
    "papaparse": "^5.5.3",
    "pinia": "^2.2.6",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.1.0",
    "smartystreets-javascript-sdk": "^7.1.0",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "vuex": "^4.1.0",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.2.0",
    "@eslint/js": "^9.15.0",
    "@playwright/mcp": "^0.0.35",
    "@playwright/test": "^1.48.2",
    "@primevue/auto-import-resolver": "^4.3.7",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.1",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.1.1",
    "eslint": "^9.15.0",
    "eslint-plugin-vue": "^9.31.0",
    "happy-dom": "^15.11.6",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "puppeteer": "^23.8.0",
    "supabase": "^2.39.2",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "unplugin-auto-import": "^0.18.4",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.1",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  }
}
